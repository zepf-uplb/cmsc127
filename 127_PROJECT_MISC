CREATE DATABASE spotify;
CREATE USER spotify_dba WITH PASSWORD 'qpwoeiruty';
GRANT ALL PRIVILEGES ON DATABASE spotify TO spotify_dba;


DROP TABLE CONTAIN;
DROP TABLE SEARCH;
DROP TABLE RECOMMEND;
DROP TABLE PLAYLIST;
DROP TABLE MUSIC;
DROP TABLE PROFILE;
DROP TABLE USER_;
DROP TABLE ADMINISTRATOR;


REVOKE ALL PRIVILEGES ON DATABASE spotify FROM spotify_dba;
DROP USER spotify_dba;
\c postgres
DROP DATABASE spotify;
